{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "functions": {
      "api/*.js": {
        "runtime": "nodejs@20.9.0"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "dest": "/"
      }
    ],
    "git": {
      "deploymentEnabled": {
        "main": false
      }
    }
  }